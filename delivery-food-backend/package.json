{
    "name": "mongodb-typescript-example",
    "version": "1.0.0",
    "description": "Welcome to this MongoDB and TypeScript sample project.  The aim of this project is to give you a working example of how you can use the power of MongoDB Atlas with TypeScript and Express to create modern web applications.",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "buildrun": "npm run build && node dist/index.js",
        "dev": "nodemon -e ts --exec \"npm run buildrun\"",
        "start": "npm run build && node dist/index.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mongodb-developer/mongodb-typescript-example.git"
    },
    "keywords": [
        "mongodb",
        "typescript",
        "express",
        "atlas"
    ],
    "author": "Luce Carter",
    "license": "Apache 2.0",
    "bugs": {
        "url": "https://github.com/mongodb-developer/mongodb-typescript-example/issues"
    },
    "homepage": "https://github.com/mongodb-developer/mongodb-typescript-example#readme",
    "dependencies": {
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "express-joi-validation": "^5.0.1",
        "joi": "^17.6.0",
        "mongodb": "^4.0.1",
        "nodemon": "^2.0.16",
        "swagger-jsdoc": "^6.2.1",
        "swagger-ui-express": "^4.4.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.13",
        "@types/node": "^16.4.7",
        "@types/swagger-jsdoc": "^6.0.1",
        "@types/swagger-ui-express": "^4.1.3",
        "prettier": "^2.3.2",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.3.5"
    }
}
